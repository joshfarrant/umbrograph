datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model File {
  id    String  @id @default(uuid())
  owner String
  data  String
  type  String
}

